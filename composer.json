{
  "name": "timostamm/orm-resource",
  "description": "Store files in a Doctrine ORM database",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "timostamm/web-resource": "^1.1.0",
    "doctrine/orm": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.23 || ^9",
    "mikey179/vfsstream": "^1.6.11",
    "symfony/cache": ">=4.4 < 6",
    "doctrine/annotations": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "TS\\Web\\Resource\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TS\\Web\\Resource\\": "tests/"
    }
  }
}
